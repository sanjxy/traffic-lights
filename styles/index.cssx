.trafficBox {
  height: 45%;
  width: 125px;
  border: 2px solid black;
  background-color: grey;
    margin-left: 50%;
    border-radius: 20px;
}

.trafficLight {
  height: 100px;
  width: 100px;
  background-color:  black;
  border-radius: 50%;
    margin: 10px;

    margin-top: 28px;
    margin-left: 13px;
}



.blink{
    animation: blinker 1s linear infinite;

}

@keyframes blinker {
  50% {
    opacity: 100;
    background-color: black;
  }
}